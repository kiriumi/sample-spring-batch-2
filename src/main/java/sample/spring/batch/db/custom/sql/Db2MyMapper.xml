<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sample.spring.batch.db.custom.dao.Db2Mapper">

	<insert id="insertSample1">
		INSERT INTO public.db2 (id, name)
		VALUES (#{id}, #{name})
	</insert>

	<update id="updateSample1" parameterType="sample.spring.batch.db.generator.dto.Db2">
		UPDATE
		public.db2
		SET
		name = #{name}
		WHERE id = #{id}
	</update>

</mapper>